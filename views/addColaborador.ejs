                                    <div class="form-group">
                                        <label class="col-md-12">Nome Completo</label>
                                        <div class="col-md-12">
                                            <input type="text" name="nomeCompleto" id="nomeCompleto" placeholder="Ex.: Marcio Cunha" required class="form-control form-control-line">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="example-email" class="col-md-12">Data de Nascimento</label>
                                        <div class="col-md-12">
                                            <input type="date"  name="dataNascimento" id="dataNascimento" required class="form-control form-control-line" name="example-email" id="example-email">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-12">
                                            Senha Numérica do Colaborador
                                            <br />

                                            <span style="font-size: 12px">Máximo 5 dígitos.</span>
                                        </label>
                                        <div class="col-md-12">
                                            <input required max="99999" name="senha" id="senha" type="number" value="number" class="form-control form-control-line">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-12">Endereço</label>
                                        <div class="col-md-12">
                                            <input required type="text" name="endereco" id="endereco" placeholder="Ex.: Cidade, Rua, Número" class="form-control form-control-line">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                            <label class="col-md-12">RG</label>
                                            <div class="col-md-12">
                                                <input required type="text" name="rg" id="rg" placeholder="Ex.: 99-999-999-9" class="form-control form-control-line">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                                <label class="col-md-12">CPF</label>
                                                <div class="col-md-12">
                                                    <input required type="text" name="cpf" id="cpf" placeholder="Ex.: 999-999-999-99" class="form-control form-control-line">
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                    <label class="col-md-12">Telefone</label>
                                                    <div class="col-md-12">
                                                        <input required type="text" name="telefone" id="telefone" placeholder="Ex.: (44) xxxxx-xxxx" class="form-control form-control-line">
                                                    </div>
                                                </div>

                                    <div class="form-group">
                                        <div class="col-sm-12">
                                            <button class="btn btn-success">Adicionar Colaborador</button>
                                        </div>
